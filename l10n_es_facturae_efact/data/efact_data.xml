<?xml version="1.0" ?>
<odoo noupdate="1">
    <record id="account_move_efact_server" model="ir.config_parameter">
        <field name="key">account.move.efact.server</field>
        <field name="value">efact.eacat.cat</field>
    </record>
    <record id="account_move_efact_port" model="ir.config_parameter">
        <field name="key">account.move.efact.port</field>
        <field name="value">22</field>
    </record>
    <record id="account_move_efact_user" model="ir.config_parameter">
        <field name="key">account.move.efact.user</field>
        <field name="value">usuario</field>
    </record>
    <record id="account_move_efact_password" model="ir.config_parameter">
        <field name="key">account.move.efact.password</field>
        <field name="value">password</field>
    </record>
    <record id="seq_account_move_efact" model="ir.sequence">
        <field name="name">Integration eFact</field>
        <field name="code">account.move.integration.face</field>
        <field name="prefix">eFACT</field>
        <field name="padding">8</field>
    </record>
    <record id="integration_efact" model="account.move.integration.method">
        <field name="name">eFACT</field>
        <field name="code">eFACT</field>
        <field name="sequence_id" ref="l10n_es_facturae_efact.seq_account_move_efact" />
    </record>
    <record id="update_efact_job" model="ir.cron">
        <field name="name">eFACT update Job</field>
        <field name="active">True</field>
        <field name="user_id" ref="base.user_root" />
        <field name="priority">8</field>
        <field name="interval_type">hours</field>
        <field name="state">code</field>
        <field name="numbercall">-1</field>
        <field
            name="model_id"
            ref="l10n_es_facturae.model_account_move_integration_log"
        />
        <field name="code">model.efact_check_history()</field>
    </record>
</odoo>
